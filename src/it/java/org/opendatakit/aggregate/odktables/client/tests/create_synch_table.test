# Set up
createUser user1

# Test that we can create a new table
createSynchronizedTable user1 people
    name STRING false
    age INTEGER false
    weight DECIMAL false

# Test that we can't create a table that already exists
createSynchronizedTable user1 people
    name STRING false
    age INTEGER false
    weight DECIMAL false

# Test that we can create a table with properties
createSynchronizedTable user1 props {'key':'value','property2':'value'}
	column1 STRING false
	column2 INTEGER true

# Clean up
deleteSynchronizedTable user1 people
deleteSynchronizedTable user1 props
deleteUser user1