<?xml version="1.0" encoding="UTF-8"?>
<queue-entries xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:noNamespaceSchemaLocation="queue.xsd">
  <queue>
    <name>default</name>
    <rate>1/s</rate>
    <max-concurrent-requests>5</max-concurrent-requests>
    <retry-parameters> <!--  give GAE one retry - aggregate manages its own retries -->
      <task-retry-limit>1</task-retry-limit>
    </retry-parameters>
  </queue>
  <queue>
    <name>upload-submission-queue</name>
    <rate>1/s</rate>
    <bucket-size>50</bucket-size>
    <max-concurrent-requests>5</max-concurrent-requests>
    <retry-parameters> <!--  give GAE one retry - aggregate manages its own retries -->
      <task-retry-limit>1</task-retry-limit>
    </retry-parameters>
  </queue>
</queue-entries>
